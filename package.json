{
  "name": "douze-prettify-logs",
  "version": "0.4.0",
  "description": "NDJSON prettifier for Pino logs generated by Douze",
  "license": "MIT",
  "main": "dist/main.js",
  "bin": {
    "douze-prettify-logs": "dist/main.js"
  },
  "author": {
    "name": "Fran√ßois Best",
    "email": "contact@francoisbest.com",
    "url": "https://francoisbest.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/franky47/douze-prettify-logs"
  },
  "keywords": [
    "douze",
    "logs",
    "ndjson",
    "prettify"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "nodemon -e ts,.env -w .env -w . -x 'cat douze.ndjson | ts-node ./src/main.ts'",
    "build:clean": "rm -rf ./dist",
    "build:ts": "tsc",
    "build": "run-s build:clean build:ts",
    "test": "jest",
    "ci": "run-s test build"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^4.0.1",
    "dayjs": "^1.8.14",
    "dotenv": "^8.2.0",
    "readline": "^1.3.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.8",
    "husky": "^3.0.2",
    "jest": "^24.8.0",
    "nodemon": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.1"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "husky": {
    "pre-push": "run-s test build"
  }
}
